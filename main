class Media {
  void play() {
    print("Playing media...");
  }
}

class Song extends Media {
  final String artist;

  Song(this.artist);

  @override
  void play() {
    print("Playing song by $artist...");
  }
}

void main() {
  // Create a Media object
  var media = Media();

  // Create a Song object
  var song = Song("The Beatles");

  // Call the play() method on each object
  media.play();
  song.play();
}
